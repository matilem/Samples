@using Aafp.Events.Admin.Helpers
<!DOCTYPE html>
<html class="no-js" lang="">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>@ViewBag.Title</title>
    <meta name="description" content="AAFP Staff tool for quickly registering people for our events.">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="apple-touch-icon-precomposed" sizes="57x57" href="~/Content/apple-touch-icon-57x57.png" />
    <link rel="apple-touch-icon-precomposed" sizes="120x120" href="~/Content/apple-touch-icon-120x120.png" />
    <link rel="apple-touch-icon-precomposed" sizes="76x76" href="~/Content/apple-touch-icon-76x76.png" />
    <link rel="apple-touch-icon-precomposed" sizes="152x152" href="~/Content/apple-touch-icon-152x152.png" />
    <link rel="icon" type="image/png" href="~/Content/favicon-96x96.png" sizes="96x96" />
    <link rel="icon" type="image/png" href="~/Content/favicon-16x16.png" sizes="16x16" />
    <link rel="icon" type="image/png" href="~/Content/favicon-128.png" sizes="128x128" />
    <meta name="application-name" content="Quick Reg"/>
    <meta name="msapplication-TileColor" content="#FFFFFF" />
    <meta name="msapplication-square70x70logo" content="~/Content/mstile-70x70.png" />
    <meta name="msapplication-square150x150logo" content="~/Content/mstile-150x150.png" />
    <meta name="msapplication-wide310x150logo" content="~/Content/mstile-310x150.png" />
    <link rel="stylesheet" href="https://code.jquery.com/ui/1.11.4/themes/flick/jquery-ui.css">
    <link href="//cdnjs.cloudflare.com/ajax/libs/select2/4.0.1/css/select2.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="@SyndicationHelper.GetCssSyndicationLink("quick-registration/style.css")">
    <script src="https://code.jquery.com/jquery-2.1.4.min.js"></script>
    <script src="https://code.jquery.com/ui/1.11.4/jquery-ui.min.js"></script>
</head>
<body id="top">
    <style>
        .select2-container {
            width: 100% !important;
        }
    </style>

    <div id="overlay" class="web_dialog_overlay"></div>

    <!--[if IE]>
        <p class="browserupgrade">You are using an <strong>unsupported</strong> browser. Please use Firefox or Safari.</p>
    <![endif]-->

    @RenderBody()
    @Scripts.Render("~/bundles/layout-js")
</body>
</html>